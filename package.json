{
  "name": "location-tracker",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "cypress:open": "wait-on tcp:3000 && cypress open",
    "test:dev": "run-p dev cypress:open",
    "dev": "vite --host 0.0.0.0",
    "build": "tsc && cross-env BUILD=enabled vite build",
    "lint": "npx eslint src",
    "test": "cross-env NODE_ENV=test jest",
    "docker": "docker-compose --file docker/development/docker-compose.yml up -d --force-recreate",
    "start": "cross-env NODE_ENV=production node server.js",
    "lint:fix": "npm run lint -- --fix",
    "heroku-postbuild": "npm run build",
    "preview": "vite preview"
  },
  "dependencies": {
    "axios": "^0.27.2",
    "body-parser": "^1.20.0",
    "compression": "^1.7.4",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.1",
    "error-handler-module": "^1.0.6",
    "express": "^4.18.1",
    "express-rate-limit": "^6.4.0",
    "helmet": "^5.1.0",
    "i18next": "^21.8.10",
    "i18next-browser-languagedetector": "^6.1.4",
    "mongoose": "^6.4.0",
    "morgan": "^1.10.0",
    "pino": "^8.1.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-i18next": "^11.17.2",
    "react-router-dom": "^6.3.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/mocha": "^9.1.1",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.29.0",
    "@typescript-eslint/parser": "^5.29.0",
    "@vitejs/plugin-react": "^1.3.0",
    "cypress": "^10.2.0",
    "eslint": "^8.2.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-jsx-a11y": "^6.6.0",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jest": "^28.1.1",
    "npm-run-all": "^4.1.5",
    "sass": "^1.53.0",
    "supertest": "^6.2.3",
    "typescript": "^4.6.3",
    "vite": "^2.9.9",
    "vite-plugin-favicon": "^1.0.8",
    "vite-plugin-i18n-resources": "^1.0.3",
    "vite-plugin-i18next-scanner": "^0.4.0",
    "wait-on": "^6.0.1"
  }
